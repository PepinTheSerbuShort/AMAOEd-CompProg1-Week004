#include <iostream>
#include <string>
using namespace std;

void ama_college_trigrade(string first_name, string last_name, 
	float prelim_quiz_1, float prelim_quiz_2, float prelim_exam, 
	float midterm_quiz_1, float midterm_quiz_2, float midterm_exam, 
	float final_quiz_1, float final_quiz_2, float final_exam)
{
	float total_score = prelim_quiz_1 + prelim_quiz_2 + prelim_exam + 
	midterm_quiz_1 + midterm_quiz_2 + midterm_exam + final_quiz_1 + 
	final_quiz_2 + final_exam;

	float grade_result = total_score / 9;

	cout << "\n\nYour name is " << first_name << " " << last_name;

	if (grade_result >= 75.00)
	{
		cout << ". You have passed your grade with " << grade_result << "%.";
	} else {
		cout << ". You have failed your grade.";
	}
}

int main()
{
	string first_name, last_name;

	float prelim_quiz_1, prelim_quiz_2, prelim_exam, 
	midterm_quiz_1, midterm_quiz_2, midterm_exam, 
	final_quiz_1, final_quiz_2, final_exam;

	cout << "Type your name; \nFirst Name: ";
	cin >> first_name;
	cout << "\nLast Name: ";
	cin >> last_name;
	cout << "\n\nType your scores from the 1st Trimester below;\nPrelim Quiz 1: ";
	cin >> prelim_quiz_1;
	cout << "\nPrelim Quiz 2: ";
	cin >> prelim_quiz_2;
	cout << "\nPrelim Exam: ";
	cin >> prelim_exam;
	cout << "\nMidterm Quiz 1: ";
	cin >> midterm_quiz_1;
	cout << "\nMidterm Quiz 2: ";
	cin >> midterm_quiz_2;
	cout << "\nMidterm Exam: ";
	cin >> midterm_exam;
	cout << "\nFinal Quiz 1: ";
	cin >> final_quiz_1;
	cout << "\nFinal Quiz 2: ";
	cin >> final_quiz_2;
	cout << "\nFinal Exam: ";
	cin >> final_exam;

	ama_college_trigrade(first_name, last_name, 
	prelim_quiz_1, prelim_quiz_2, prelim_exam, 
	midterm_quiz_1, midterm_quiz_2, midterm_exam, 
	final_quiz_1, final_quiz_2, final_exam);

	return 0;
}
